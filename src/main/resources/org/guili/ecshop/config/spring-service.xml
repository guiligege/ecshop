<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
xmlns:aop="http://www.springframework.org/schema/aop"
xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/aop 
           http://www.springframework.org/schema/aop/spring-aop-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
           
    <!-- service -->
	<bean id="testBusiness" class="org.guili.ecshop.business.TestBussiness">
			<property name="testDao" ref="ITestTableDaoImpl"></property>
			<property name="semiconductorDao" ref="semiconductorDaoImpl"></property>
	</bean>
	<bean id="semiconductorService" class="org.guili.ecshop.business.impl.spider.SemiconductorService">
			<property name="semiconductorDao" ref="semiconductorDaoImpl"></property>
	</bean>
	<!-- 3个服务类 -->
	<bean id="digikeySpiderServiceImpl" class="org.guili.ecshop.business.impl.spider.DigikeySpiderServiceImpl">
			<property name="semiconductorService" ref="semiconductorService"></property>
	</bean>
	 
	<bean id="mouserSpiderServiceImpl" class="org.guili.ecshop.business.impl.spider.MouserSpiderServiceImpl">
			<property name="semiconductorService" ref="semiconductorService"></property>
	</bean>
	<bean id="avnetSpiderServiceImpl" class="org.guili.ecshop.business.impl.spider.AvnetSpiderServiceImpl">
			<property name="semiconductorService" ref="semiconductorService"></property>
	</bean>
	
	
	<!-- 评价系统 -->
	<bean id="taobaoProductEvaluate" class="org.guili.ecshop.business.impl.evaluate.TaobaoProductEvaluate">
	</bean>
	<bean id="tmallProductEvaluate" class="org.guili.ecshop.business.impl.evaluate.TmallProductEvaluate">
		<property name="tmallAnalyzeDao" ref="tmallAnalyzeDao"></property>
	</bean>
	<bean id="prodcutEvaluateFactory" class="org.guili.ecshop.business.impl.evaluate.ProdcutEvaluateFactory">
		<property name="taobaoProductEvaluate" ref="taobaoProductEvaluate"></property>
		<property name="tmallProductEvaluate" ref="tmallProductEvaluate"></property>
	</bean>
	
</beans>